{
  "name": "react-lib-boilerplate",
  "version": "0.0.1",
  "description": "1. git clone https://github.com/christoferolaison/react-lib-boilerplate.git -o upstream . 2. git remote add origin https://github.com/{username}/{repo}.git 3. git push --set-upstream origin master (git checkout --ours PATH_TO_FILE)",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "scripts": {
    "build": "babel src -d lib --ignore spec.js,test.js",
    "dev": "watch 'npm run build' src",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christoferolaison/react-lib-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/christoferolaison/react-lib-boilerplate/issues"
  },
  "homepage": "https://github.com/christoferolaison/react-lib-boilerplate#readme",
  "babel": {
    "presets": [
      "env",
      "stage-3",
      "react"
    ]
  },
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.5.0",
    "eslint-config-christofer": "^2.1.0",
    "watch": "^1.0.2"
  }
}
